<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to='tortuescript'>
	<inherits name="com.google.common.collect.Collect" />
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.inject.Inject" />
	<inherits name='com.google.gwt.activity.Activity' />
	<inherits name="com.google.gwt.logging.Logging" />

	<entry-point class='fr.salvadordiaz.gwt.tortuescript.client.TortueScript' />

	<set-property name="user.agent" value="safari,gecko1_8" />
	<set-property name="gwt.logging.popupHandler" value="DISABLED" />

	<extend-property name="locale" values="en,fr" />
	<set-property-fallback name="locale" value="fr" />

	<set-property name="locale" value="en,fr" />
</module>
